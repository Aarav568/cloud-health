<%- include("partials/sideNav.ejs") %>

<div class="content" style="padding: 10px">
    <div class="fingerprint">
        <img src="https://www.pngall.com/wp-content/uploads/2016/06/Fingerprint-Free-Download-PNG.png" height="500"
            width="500" alt="">
        <h3>Please wait while we scan your Fingerprint...</h3>
    </div>
    <h2 style="display: none">Sorry, you don't have any medical history</h2>
</div>

<script>
    window.onload = function () {
        setTimeout(function () {
            document.querySelector(".fingerprint").style.display = "none"
            setTimeout(function () {
                document.querySelector("h2").style.display = "block"
            }, 1000);
        }, 3000);
    };
</script>